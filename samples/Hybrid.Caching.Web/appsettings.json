{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "HybridCaching": {
        "InstanceName": "Sample",
        "Configuration": "localhost:6379,allowAdmin=true,abortConnect=false",
        "DefaultExpiration": "01:00:00",
        "Locking": {
            "Retry": "00:00:01",
            "Expiry": "00:00:30",
            "Wait": "00:00:10"
        }
    }
}
